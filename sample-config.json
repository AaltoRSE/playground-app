{
    "allowed_ip_addresses": ["IP1","IP2"],
    "host": "0.0.0.0",
    "port": 6080,
    "https_chain": "/etc/letsencrypt/live/your.domain/fullchain.pem",
    "https_key": "/etc/letsencrypt/live/your.domain/privkey.pem",
    "eu_login_local_url": "https://your.domain:6080/oauth/login",
    "eu_login_server_url": "https://ecas.acceptance.ec.europa.eu/cas",
